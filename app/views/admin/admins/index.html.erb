<div class="row">
  <h1>Admins</h1>
    <table class="dataTable table table-striped table-bordered">
      <thead>
        <tr>
          <th>Email</th>
          <th>Created</th>
          <th>Updated</th>
          <th>Edit</th>
          <th>Delete</th>
        </tr>
      </thead>
      <tbody>
        <% @admins.each do |admin| %>
          <tr>
            <td><%= admin.email %></td>
            <td><%= admin.created_at %></td>
            <td><%= admin.updated_at %></td>
            <td><%= link_to 'Edit', edit_admin_admin_path(admin) %></td>
            <td><%= link_to 'Destroy', [:admin, admin], method: :delete, data: { confirm: 'Are you sure?' } %></td>
          </tr>
        <% end %>
      </tbody>
    </table>
    <br>

    <%= button_to 'New Admin', { action: "new"}, method: "get", class: "btn btn-default" %>

</div>